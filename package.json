{
  "name": "remath",
  "version": "1.0.0",
  "description": "A reactive spreadsheet-like library",
  "main": "lib/Remath.js",
  "scripts": {
    "test": "npm run quick-build && npm run tape",
    "jest": "jest --watch",
    "quick-build": "babel src --out-dir lib",
    "tape": "tape test/**/*.js | faucet",
    "prepublish": "npm run quick-build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/trevorhanus/recalc.git"
  },
  "keywords": [
    "calculations",
    "math",
    "engineering",
    "mathematics",
    "spreadsheet"
  ],
  "author": "Trevor Hanus <trevorhanus@gmail.com> (http://github.com/trevorhanus)",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^16.0.4",
    "@types/sinon": "^1.16.34",
    "babel-cli": "^6.6.5",
    "babel-plugin-transform-class-properties": "^6.6.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-destructuring": "^6.6.5",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.1.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "faucet": "0.0.1",
    "jest-cli": "^18.1.0",
    "sinon": "^1.17.7",
    "tape": "^4.5.1",
    "tape-catch": "^1.0.4"
  },
  "dependencies": {
    "@types/es6-shim": "^0.31.32",
    "@types/mathjs": "0.0.32",
    "@types/underscore": "^1.7.36",
    "@types/uuid": "^2.0.29",
    "check-types": "^6.0.0",
    "mathjs": "^3.1.0",
    "mobx": "^3.0.0",
    "node-uuid": "^1.4.3",
    "numeral": "^1.5.3",
    "randomstring": "^1.1.5",
    "typescript": "^2.1.5",
    "underscore": "^1.8.3",
    "uuid": "^3.0.1"
  },
  "jest": {
    "transform": {
      ".*": "<rootDir>/preprocessor.js"
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testRegex": "__test__/.*\\.(ts)$"
  }
}
